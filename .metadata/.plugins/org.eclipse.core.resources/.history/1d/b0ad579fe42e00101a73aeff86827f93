<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>내장 객체 - request</title>
</head>
<body>
	<%	
		String hobby = request.getParameter("hobby")
		String[] hobbies = request.getParameterValues("hobby");
		for(int i = 0; i < hobbies.length; i++) {
	%>
		<h5><%= hobbies[i] %></h5>
	<%
		}
	%>
	<hr>
	<%= hobby %>
</body>
</html>